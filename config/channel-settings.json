  "channelAuthSchema": {
    "authType": "oauth",
    "configDef": {
      "configModel": {
        "type": "object",
        "title": "oAuth Config",
        "properties": {
          "shop": {
            "title": "Shop URL",
            "type": "string",
            "pattern": "^[\\w.+\\-]+\\.myshopify\\.com$",
            "validationMessage": "Must be a valid Shopify Shop URL"
          },
          "access_token":{
            "type": "string"
          }
        },
        "required": [
          "shop",
          "access_token"
        ]
      },
      "configView": [
        {
          "key": "shop",
          "title": "Shop URL",
          "placeholder":"<my-store>.myshopify.com",
          "isMainField": true
        }
      ],
      "configValues": {}
    },
    "oauthAuthorizeUrl": "https://{{shop}}/admin/oauth/authorize?client_id={{client_id}}&scope=read_orders,read_products,read_inventory,read_customers,read_fulfillments,read_shipping,read_content,write_orders,write_products,write_inventory,write_customers,write_fulfillments,write_shipping,write_content&redirect_uri=https://localhost/account/v1/applications/getAccessToken&state={{state}}",
    "oauthTokenRequest": {
      "verb": "POST",
      "url": "https://{{shop}}/admin/oauth/access_token",
      "headers": {
        "Content-Type": "application/json"
      },
      "body": {
        "client_id": "{{client_id}}",
        "client_secret": "{{client_secret}}",
        "code": "{{code}}"
      }
    },
    "oauthTokenResponse": {
      "access_token": "access_token",
      "scope": "scope"
    },
    "apps": [
      {
        "applicationRegistryID": "100000000000000000000003",
        "appName": "automation-manager"
      }
    ],
    "oauthUserRequest": {
      "verb": "GET",
      "url": "https://{{shop}}/admin/shop.json",
      "headers": {
        "Content-Type": "application/json",
        "X-Shopify-Access-Token": "{{access_token}}"
      }
    },
    "oauthUserResponse": {
      "user": {
        "firstName": "shop.shop_owner",
        "emailAddress": "shop.email"
      },
      "account": {
        "company": "shop.name"
      }
    },
    "oauthIdentifier": "{{shop}}",
    "oauthRequestVerification": {
      "algorithm": "sha256",
      "key": "client_secret",
      "digest": "hmac",
      "encoding": "hex",
      "parameters": [
        {"index": 0, "key": "code", "ref": "code"},
        {"index": 1, "key": "shop", "ref": "shop"},
        {"index": 2, "key": "state", "ref": "state"},
        {"index": 3, "key": "timestamp", "ref": "timestamp"}
      ]
    }
  }
}
